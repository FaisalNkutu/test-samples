		amqpEnabledRpgPrograms = parseCSVConfig(CONFIG_KEY_AMQP_ENABLED_RPG_PROGRAMS);
		if(!amqpEnabledRpgPrograms.contains("WEBBI120") && CONFIG_KEY_REST_ENABLED_PROGRAMS.equalsIgnoreCase("True")) {		
			if (DatabaseConstant.TRADING_PARTNER_CPDN.equals(medisAccount.getTradingPartnerCode())){
				request.setAttribute(PARAM_ACTION, "sendMessage");
				actionHandlerForSendMessage(request, response);
				//save not allowed for MDP.Block the action if scripted or programmer.
				return;
			}
		}
